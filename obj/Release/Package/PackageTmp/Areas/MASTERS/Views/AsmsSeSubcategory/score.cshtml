@model IEnumerable<IEM.Areas.MASTERS.Models.SESubCategoryModel>
@using IEM.Common;
@{
    var grid = new WebGrid(source: Model, rowsPerPage: 10,canSort:false, ajaxUpdateContainerId: "grid");
    CmnFunctions cmn = new CmnFunctions();    
}
@using (Html.BeginForm())
{
    <div>
        <div >           
            @grid.GetHtml(
      tableStyle: "tableSmall table-bordered table-hover table-responsive",
            columns: grid.Columns
            (                 
                 grid.Column("seRateName",cmn.Sorter("seRateName", "Evaluation Rate", grid)),
                 grid.Column("seRateScore", cmn.Sorter("seRateScore", "Rate Score", grid))
             ))
        </div>
    </div>
}