    function DeleteProdServ() {
        var prodservId = '@Model.productGid';
        var prodgidnew = parseInt($('#hideditgid').val());
        jConfirm("Do you want to delete this?", "Message", function (r) {
            if (r == true) {
                var objdel =
                {
                    "productGid": prodgidnew,
                };
                $.post('../FbProductService/Delete', objdel,
                     function (data) {
                         if (data == 0) {
                             jAlert("Please Delete Child Product/Service First", "Message");
                             return false;
                         }
                         else {
                             jAlert("Record Deleted Successfully", "Message", function (e) {
                                 if (e == true) {
                                     location = '../FbProductService/Index';
                                 }
                             });
                         }
                     }, 'json');
            }
        });
    }

