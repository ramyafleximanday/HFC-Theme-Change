@{
    ViewBag.Title = "Testmail";
    Layout = "~/Views/Shared/LayoutEmpty.cshtml";
}
<link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/Rmax_Site.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>

<br /><br />
<br /><br />
<style>
    #body {
        height: 100%;
        width: 100%;
        padding: 0;
        margin: 0;
    }

    #full-screen-background-image {
        z-index: -999;
        min-height: 100%;
        min-width: 1024px;
        width: 100%;
        height: auto;
        position: fixed;
        top: 0;
        left: 0;
    }

    #wrapper {
        position: relative;
        width: 800px;
        min-height: 400px;
        margin: 100px auto;
        color: #333;
    }
</style>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div align="center" id="body">
        <img alt="full screen background image" id="full-screen-background-image" src="data:image/jpg;base64," />
        <div align="center" id="wrapper">
            <p align="center">
                <table style="width:100%;" class="myTableStyleTabNew tableSmall table-responsive tablefix">
                    <thead>
                        <tr>
                            <th colspan="4"><b> Development Mail Testing </b> </th>
                        </tr>
                    </thead>
                    <tbody style="background-color: aliceblue;">
                        <tr>
                            <td style="width: 35%;">
                                Module :   <input type="text" id="Module" class="textboxStyle" name="Module" value="EOW" />
                            </td>
                            <td>
                                Transaction Type :  <input type="text" class="textboxStyle" id="TransactionType" name="TransactionType" value="SUPPLIERINVOICE" />
                            </td>
                            <td>
                                Queue Gid : <input type="text" id="Gid" class="textboxStyle" name="Gid" value="39636" />
                            </td>
                            <td style="width: 15%;">
                                Request :  <input type="text" class="textboxStyle" id="request" name="request" value="S" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Send mail id : <input class="textboxStyle" type="text" id="sendmailid" name="sendmailid" value="thirumalaivasan9245@gmail.com" />
                            </td>
                            <td>
                                Send pwd : <input class="textboxStyle" type="password" id="sendpw" name="sendpw" value="thirumalai@123" />
                            </td>
                            <td>
                                Smpt Mail server :  <input type="text" class="textboxStyle" id="smptMailserver" name="smptMailserver" value="smtp.gmail.com" />
                            </td>
                            <td>
                                Smpt Mail Port : <input type="text" class="textboxStyle" id="smptMailPort" name="smptMailPort" value="587" />
                            </td>
                        </tr>
                        <tr>
                            <td valign="top" colspan="4">
                                To Mail ID :
                                <br />
                                <textarea id="txtemail" name="txtxname" style="width: 100%; height:60px;" maxlength="250" class="textboxStyle">thirumalai@flexicodeindia.com</textarea>
                                @*<input type="text" id="txtemail" class="textboxStyle" name="txtxname" value="thirumalai@flexicodeindia.com" />*@
                            </td>
                        </tr>
                        <tr>
                            <td>
                                Mail Enable SSL  :
                                <br />
                                <input type="radio" name="Enablessl" id="rblTrue" value="True" checked="checked" />&nbsp;&nbsp;True
                                &nbsp;&nbsp; <input type="radio" name="Enablessl" id="rblFalse" value="False">&nbsp;&nbsp;False
                            </td>
                            <td colspan="3"></td>
                        </tr>
                        <tr>
                            <td colspan="4" align="center">
                                <br />
                                <center>
                                    <input type="submit" id="btnsavexpNNgfhg" class="btn btn-primary" value="Sendmail" />&nbsp;&nbsp;&nbsp;
                                    <input type="button" id="btnclear" class="btn btn-warning" value="Clear" />
                                </center>
                                <br />
                            </td>
                        </tr>
                    </tbody>
                </table>
                <label style="color: red;">@Session["error"]</label>
                <br /><br /><br /><br />
            </p>
        </div>
    </div>

}
