@model IEM.Areas.ASMS.Models.SupAttachment
           @{ 
               if (Session["PageMode"] != null)
               {
                   ViewBag.PageMode = (string)Session["PageMode"];
                   //if (ViewBag.PageMode == "3")
                   //{
                   //    ViewBag.IsAllowApproverToEdit = (string)Session["IsAllowApproverToEdit"];
                   //    if (ViewBag.IsAllowApproverToEdit == "1")
                   //    {
                   //        ViewBag.PageMode = "2";
                   //    }
                   //}
               }              
           }
<script type="text/javascript">
    var curpagemode;
    $(document).ready(function () {
        $('#tabs ul').tabs({
            select: function (event) {
                $("#wgSupAttachment").load("../Onboarding/SupAttachmentDetails?" + new Date().getTime());
            }
        });
        curpagemode = '@ViewBag.PageMode';
        if (curpagemode == "3" || curpagemode == "5") {
            $("#supattachment").css("display", "none");
           
        }
        
    });
    $("#btnNextAttachment").live("click", function () {
        $("#tabs").tabs({ disabled: [10] });
        $('#tabs').tabs('select', 6);
});
    $("#btnPrevAttachment").live("click", function () {
        $("#tabs").tabs({ disabled: [10] });
        $('#tabs').tabs('select', 4);
});
</script>
<div>
    <div id="divSupAttachmentDetails">
        <fieldset>
            <legend>SUPPLIER ATTACHMENTS</legend>
           
                <div id="supattachment">
                    @Html.Partial("SupAttachmentFields")
                </div>
                <div>
                    @Html.Partial("SupAttachmentDetails")
                </div>
           
        </fieldset>
    </div>
    <br />
    <div style="text-align:right;padding-right:25px;" class="divforbutton">
        <button id="btnPrevAttachment" class="btn btn-primary"><span style="color:white;" class="glyphicon glyphicon-backward"></span> Prev</button>
        <button id="btnNextAttachment" class="btn btn-primary">Next <span style="color:white;" class="glyphicon glyphicon-forward"></span></button>
    </div>
</div>
