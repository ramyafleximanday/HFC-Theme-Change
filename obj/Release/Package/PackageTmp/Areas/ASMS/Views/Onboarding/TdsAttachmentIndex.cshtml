@model IEM.Areas.ASMS.Models.SupplierTaxDetails
@{
    if (Session["PageMode"] != null)
    {
        ViewBag.PageMode = (string)Session["PageMode"];
        if (ViewBag.PageMode == "3")
        {
            ViewBag.IsAllowApproverToEdit = (string)Session["IsAllowApproverToEdit"];
            if (ViewBag.IsAllowApproverToEdit == "1")
            {
                ViewBag.PageMode = "2";
            }
        }
    }
    if (TempData["tdsattachmentviewmode2"] != null)
    {
        ViewBag.tdsattachmentviewmode2 = (string)TempData["tdsattachmentviewmode2"];
    }
}
<script type="text/javascript">
    var viewmode1;
  
    $(document).ready(function () {
        viewmode1 = '@ViewBag.tdsattachmentviewmode2';
        $("#wgridTdsAttachment td:nth-child(1),#wgridTdsAttachment th:nth-child(1)").hide();
        $("#wgridTdsAttachment td:nth-last-child(1)").show();
        if (viewmode1 == "view") {
            $("#tdsattachment").css("display", "none");
            $("#wgridTdsAttachment td:nth-child(8),#wgridTdsAttachment th:nth-child(8)").hide();
        }
        else if (viewmode1 == "edit") {
            $("#tdsattachment").css("display", "block");
            $("#wgridTdsAttachment td:nth-child(8),#wgridTdsAttachment th:nth-child(8)").show();
        }
    });
   
</script>
<div>
    <div id="divTdsAttachmentDetails">
        <fieldset>
            <legend>TAX SUBTYPE ATTACHMENTS</legend>
            <center>
                <div id="tdsattachment" style="display:none;">
                    @Html.Partial("~/Areas/ASMS/Views/Onboarding/TdsAttachmentFields.cshtml")
                </div>
                <div>
                    @Html.Partial("~/Areas/ASMS/Views/Onboarding/TdsAttachmentDetails.cshtml")
                </div>
            </center>
        </fieldset>
    </div>
   
</div>
