<!--ECF DETILS-->
@{
    string content = @Model;
    string ECFId = content.Split('-')[0];
    string DocType = content.Split('-')[1];

    string IsChecker = content.Split('-')[2];
  
}
<input type="hidden" id="hfDocSubType" value="@DocType"/>
@if (DocType == "1")
{
    @Html.Partial("NonTravelClaim", @IsChecker)
}
else if (DocType == "2")
{
    @Html.Partial("LocalConveyance", @IsChecker)
}
else if (DocType == "3")
{
    @Html.Partial("TravelClaim", @IsChecker)
}
else if (DocType == "4" || DocType == "10" || DocType == "11" || DocType == "13")
{
    @Html.Partial("SupplierInvoice", @IsChecker)
}
else if (DocType == "5")
{
    @Html.Partial("DSASupplier", @IsChecker)
}
else if (DocType == "6" || DocType == "7" || DocType == "9" || DocType == "12")
{
    @Html.Partial("AdvanceRequest", @IsChecker)
}
else if (DocType == "8")
{
    @Html.Partial("PettyCash", @IsChecker)
}