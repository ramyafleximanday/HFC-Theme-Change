@model IEM.Areas.ASMS.Models.SupplierTaxDetails
@{
    if (TempData["tdsviewmode"] != null)
    {
        ViewBag.tdsviewmode1 = (string)TempData["tdsviewmode"];
        TempData["tdsviewmode1"] = ViewBag.tdsviewmode1;
    }
    //if (TempData["headername"] != null)
    //{
    //    ViewBag.headername = (string)TempData["headername"];
    //    ViewBag.headername = ViewBag.headername + " DETAILS";
    //}
}
<div>
    <div id="divTdsDetails">
            <fieldset>
                <legend></legend>
                <center>
                    <input type="hidden" value="@ViewBag.tdsviewmode1" name="txtViewbagModeTds" id="txtViewbagModeTds">
                    <div id="tdsdiv">
                        @Html.Partial("~/Areas/ASMS/Views/Onboarding/TdsFields.cshtml")
                    </div>
                    <div id="tdsgrid">
                        @Html.Partial("~/Areas/ASMS/Views/Onboarding/TdsDetails.cshtml")
                    </div>
                </center>
            </fieldset>
    </div>
</div>
<div id="divTdsPopUp1"></div>
<div id="divTdsAttachmentPopup"></div>
<script type="text/javascript">
   
    var objDialogTdsAttachment;
    $(document).ready(function () {
        $("#hidTdsAttachment").val("");
        objDialogTdsAttachment = $("[id$='divTdsAttachmentPopup']");
        objDialogTdsAttachment.dialog({
            autoOpen: false,
            closeOnEscape: false,
            modal: true,
            show: 'fade-in',
            close: 'fade-out',
            async: false,
            cache: false

        });

    });
    var viewmode;
    $("#wgridTds td:nth-child(1),#wgridTds th:nth-child(1)").hide();
    $("#wgridTds td:nth-last-child(1)").show();
    $(document).ready(function () {
       viewmode = $("#txtViewbagModeTds").val();
        if ($.trim(viewmode) == "view") {
            $("#tdsdiv").css("display", "none");
           // $("#wgridTds td:nth-child(12),#wgridTds th:nth-child(12)").hide();
        }
        else if ($.trim(viewmode) == "edit") {
            $("#tdsdiv").css("display", "block");
        }
     
    });
    function DummyFunction14() {
        $("#wgridTds td:nth-child(1),#wgridTds th:nth-child(1)").hide();
        $("#wgridTds td:nth-last-child(1)").show();
        if ($.trim(viewmode) == "view") {
            $("#tdsdiv").css("display", "none");
           // $("#wgridTds td:nth-child(12),#wgridTds th:nth-child(12)").hide();
        }
        else if ($.trim(viewmode) == "edit") {
            $("#tdsdiv").css("display", "block");
        }
    }
  
</script>