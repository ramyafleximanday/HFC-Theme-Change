@model IEM.Areas.ASMS.Models.SupplierTaxDetails
@{
    
}
<div>
    <div id="divTaxAttachmentDetails">
        <fieldset>
            <legend>TAX ATTACHMENTS</legend>
            <center>
                <div id="taxattachmentdiv">
                    @Html.Partial("~/Areas/ASMS/Views/Onboarding/TaxAttachmentFields.cshtml")
                </div>
                <div id="taxattachmentgrid">
                    @Html.Partial("~/Areas/ASMS/Views/Onboarding/TaxAttachmentDetails.cshtml")
                </div>
            </center>
        </fieldset>
    </div>
</div>
<script type="text/javascript">
    var viewmode = '@ViewBag.taxattachmentviewmode';
    $("#wgridTaxAttachment td:nth-child(1),#wgridTaxAttachment th:nth-child(1)").hide();
    $("#wgridTaxAttachment td:nth-last-child(1)").show();
    $(document).ready(function () {
        $("#wgridTaxAttachment td:nth-child(1),#wgridTaxAttachment th:nth-child(1)").hide();
        $("#wgridTaxAttachment td:nth-last-child(1)").show();
        if (viewmode == "view") {
            $("#taxattachmentdiv").css("display", "none");
            $("#wgridTaxAttachment td:nth-child(8),#wgridTaxAttachment th:nth-child(8)").hide();
        }
        else if (viewmode == "edit") {
            $("#taxattachmentdiv").css("display", "block");
            $("#wgridTaxAttachment td:nth-child(8),#wgridTaxAttachment th:nth-child(8)").show();
        }
    });
    function DummyFunction20() {
        $("#wgridTaxAttachment td:nth-child(1),#wgridTaxAttachment th:nth-child(1)").hide();
        $("#wgridTaxAttachment td:nth-last-child(1)").show();
        if (viewmode == "view") {
            $("#taxattachmentdiv").css("display", "none");
            $("#wgridTaxAttachment td:nth-child(8),#wgridTaxAttachment th:nth-child(8)").hide();
        }
        else if (viewmode == "edit") {
            $("#taxattachmentdiv").css("display", "block");
            $("#wgridTaxAttachment td:nth-child(8),#wgridTaxAttachment th:nth-child(8)").show();
        }
    }
</script>