<script type="text/javascript">
    var objDialog3;
    $(document).ready(function () {
        
        $("#CustomerSearchPanel").hide();
        $("#ServiceSearchPanel").hide();
        $("#ClientSearchPanel").hide();
        objDialog3 = $("[id$='ForPartialViewCustomer']");
        objDialog3.dialog({
            autoOpen: false,
            async: false,
            cache: false,
            modal: true,
            show: 'fade-in',
            close: 'fade-out'
            //   resizable: false
            //  close: function (ev, ui) { $(this).close(); }
        });
        
       
        
    });
    
    function NewCustomer() {
        objDialog3.load('../Onboarding/CreateCustomer?' + new Date().getTime());
        objDialog3.dialog({ title: 'Add New Customer', width: 470, height: 370 });
        objDialog3.dialog("open");
    }

    function EditCustomer(id) {
        objDialog3.load('../Onboarding/EditCustomer?id=' + id + '&viewfor=edit&' + new Date().getTime());
        objDialog3.dialog({ title: 'Edit Customer', width: 470, height: 370 });
        objDialog3.dialog("open");
    }
    function ViewCustomer(id) {
        objDialog3.load('../Onboarding/EditCustomer?id=' + id + '&viewfor=view&' + new Date().getTime());
        objDialog3.dialog({ title: 'View Customer', width: 470, height: 370 });
        objDialog3.dialog("open");
    }
    function DeleteCustomer(id) {
        objDialog3.load('../Onboarding/EditCustomer?id=' + id + '&viewfor=delete&' + new Date().getTime());
        objDialog3.dialog({ title: 'Delete Customer', width: 470, height: 370 });
        objDialog3.dialog("open");

    }

    function GotoIndexCustomer() {
        window.parent.jQuery('#ForPartialViewCustomer').dialog('close');
    }

    function ShowSearchOptions() {
        if ($("#_CustomerNameSearch").val() != "" || $("#_CustomerServiceNameSearch").val() != ""
            || $("#_CustomerContactPersonSearch").val() != "" || $("#_CustomerAgeOfProductSearch").val() != "" || $("#_CustomerMobileNoSearch").val() != "" || $("#_CustomerPhoneNoSearch").val() != "") {
            $("#wgCustomer").load("../onboarding/customerdetails?" + new Date().getTime());
            $("#_CustomerNameSearch").val("");
            $("#_CustomerServiceNameSearch").val("");
            $("#_CustomerContactPersonSearch").val("");
            $("#_CustomerMobileNoSearch").val("");
            $("#_CustomerPhoneNoSearch").val("");
            $("#_CustomerAgeOfProductSearch").val("");
        }
        $("#CustomerSearchPanel").slideToggle(500);
    }
    function SearchCustomer() {
        var custname = $("#_CustomerNameSearch").val();
        var custservicename = $("#_CustomerServiceNameSearch").val();
        var custcontactperson = $("#_CustomerContactPersonSearch").val();
        var custmobile = $("#_CustomerMobileNoSearch").val();
        var custphone = $("#_CustomerPhoneNoSearch").val();
        var ageogproduct = $("#_CustomerAgeOfProductSearch").val();
        var customersmodelSearch = {
            "_CustomerName": custname,
            "_CustomerServiceName": custservicename,
            "_CustomerContactPerson": custcontactperson,
            "_CustomerMobileNo": custmobile,
            "_CustomerPhoneNo": custphone,
