@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Content/Rmax_Site.css")" rel="stylesheet" type="text/css" />

<style type="text/css">
    dl {
        margin-left: 5%;
    }

        dl dd {
            text-indent: 30px;
            font-size: 13px;
        }

        dl dt a {
            font-size: 15px;
            text-decoration: none;
        }

            dl dt a:hover {
                font-size: 15px;
                text-decoration: none;
                color: #0094ff;
            }
</style>

<div class="pageHeadingLayout">
    <span class="pageHeading">Raising Request</span>
    <div style="text-align:right; margin-top:-20px;">
        <button style="color:white;" type="button" id="btnDashboard" name="btnDashboard" value="Dashboard" class="btn btn-primary"><span style="color:white;" class="glyphicon glyphicon-dashboard"></span> Dashboard</button>
    </div>
</div>

<div class="well">
    <dl>
        <dt>
            <a href="../Onboarding/Index?pagefor=1&isnewsupplier=true"><span style="padding-right:10px;" class="glyphicon glyphicon-hand-right"></span>ONBOARDING</a>
        </dt>
        <dd>
            To on-board a new Supplier.
        </dd>
        <dt>
            <a href="../Modification/Index?queuefor=renewal&fromlink=renewal"><span style="padding-right:10px;" class="glyphicon glyphicon-hand-right"></span>RENEWAL </a>
        </dt>
        <dd>
            To renew a Supplier whose contract has expired.
        </dd>
        <dt>
            <a href="../SupplierActivationQueue/ActivationQueue"><span style="padding-right:10px;" class="glyphicon glyphicon-hand-right"></span>ACTIVATION </a>
        </dt>
        <dd>
            To activate a Supplier who is in De-active status.
        </dd>
        <dt>
            <a href="../SupplierDeActivationQueue/SupplierDeActivation"><span style="padding-right:10px;" class="glyphicon glyphicon-hand-right"></span>DE - ACTIVATION </a>
        </dt>
        <dd>
            To de-activate a Supplier who is in Active status.
        </dd>
        <dt>
            <a href="../Modification/Index?queuefor=edit&fromlink=modification"><span style="padding-right:10px;" class="glyphicon glyphicon-hand-right"></span>MODIFICATION </a>
        </dt>
        <dd>
            To modify Supplier details.
        </dd>
        <dt>
            <a href="../GSTModification/Index?queuefor=gstedit&fromlink=gstmodification"><span style="padding-right:10px;" class="glyphicon glyphicon-hand-right"></span>GST MODIFICATION </a>
        </dt>
        <dd>
            To modify GST and E-Invoicing details. @*Ramya modified*@
        </dd>
        @*<dt>
                <a href="#"><span class="glyphicon glyphicon-hand-right"></span>&nbsp; NEW ACTIVITY </a>
            </dt>
            <dd>
                To add an activity for a Supplier (System shall allow only VMU Checker to add an activity) .
            </dd>*@
    </dl>
</div>
<script type="text/javascript">
    $("#btnDashboard").live('click', function () {
        location = '../AsmsMain/Dashboard';
    });
</script>