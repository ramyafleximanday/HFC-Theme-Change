                {
                <tr>
                    <td style="min-width: 80px; max-width: 80px; padding-left: 10px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.Type</span>
                    </td>
                    <td style="min-width: 180px; max-width: 180px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.ProductServiceGroup</span>
                    </td>
                    <td style="min-width: 180px; max-width: 180px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.ProductServiceName</span>
                    </td>
                    <td style="min-width: 180px; max-width: 180px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.ProductServiceDesc</span>
                    </td>
                    <td style="min-width: 80px; max-width: 80px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.UOM</span>
                    </td>
                    <td style="min-width: 80px; max-width: 80px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.Qty</span>
                    </td>
                    <td style="min-width: 100px; max-width: 100px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.Amount</span>
                    </td>
                    <td style="min-width: 100px; max-width: 100px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.GLCode</span>
                    </td>
                    <td style="min-width: 140px; max-width: 140px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.CC@item.CC@item.CC</span>
                    </td>
                    <td style="min-width: 100px; max-width: 100px; text-align: left; border-bottom: 1px solid black;">
                        <span>@item.BudgetedLine</span>
                    </td>
                </tr>
                }
            </table>
        </td>
    </tr>
    }
    <tr>
        <td colspan="2" style="width:100%; padding:3px 5px;">
            <span>CBF Justification</span>
            <br />
            <textarea rows="2" class="txtboxStyleBig" style="width:100%; background-color: white; border-radius: 4px;">@Model.CBFDoc.CBFJustification</textarea>
        </td>
    </tr>
    <tr>
        <td colspan="2" style="width:100%; padding:3px 5px;">
            <span>Previous Approver Code/Name</span>
            <br />
            <input type="text" value="@Model.CBFDoc.PreviousApprover"
                   class="textboxStyleBig" style="width:100%;" />
        </td>
    </tr>
    <tr>
        <td colspan="2" style="width:100%; padding:3px 5px;">
            <span>Previous Approver Remarks</span>
            <br />
            <textarea rows="2" class="txtboxStyleBig" style="width:100%; background-color: white; border-radius: 4px;">@Model.CBFDoc.PreviousApproverComment</textarea>
        </td>
    </tr>
</table>
