        <td style="width:50%; padding:0px 5px;">
            <span>PAR / PR #</span>
            <br />
            <input type="text"
                   class="textboxStyleBig" value="@Model.CBFDoc.PARPRNo" style="width:100%; " />
        </td>
    </tr>
    <tr>
        <td style="width:50%; padding:0px 5px;">
            <span>PAR / PR  Amount</span>
            <br />
            <input type="text" value="@Model.CBFDoc.PARPRAmt"
                   class="textboxStyleBig" style="width:100%; " />
        </td>
        <td style="width:50%; padding:0px 5px;">
            <span>CBF Approval</span>
            <br />
            <input type="text"
                   class="textboxStyleBig" value="@Model.CBFDoc.CBFApproval" style="width:100%; " />
        </td>
    </tr>
    <tr>
        <td style="width:50%; padding:0px 5px;">
            <span>Budgeted</span>
            <br />
            <input type="text" value="@Model.CBFDoc.Budgeted"
                   class="textboxStyleBig" style="width:100%; " />
