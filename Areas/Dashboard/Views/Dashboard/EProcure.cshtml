@model IEM.Areas.Dashboard.Models.EProcurePDFModel

<!-- CBF -->
@if (Model.DocType != null && Model.DocType == "CBF")
{
<table class="myTableStyleTabNew" style="width:100%;">
    <tr>
        <td style="width:50%; padding:0px 5px;">
            <span>CBF No</span>
            <br />
            <input type="text" value="@Model.CBFDoc.CBFNo"
                   class="textboxStyleBig" style="width:100%; " />
        </td>
        <td style="width:50%; padding:0px 5px;">
            <span>CBF Date</span>
            <br />
            <input type="text"
                   class="textboxStyleBig" value="@Model.CBFDoc.CBFDate" style="width:100%; " />
        </td>
    </tr>
    <tr>
        <td colspan="2" style="width:100%; padding:0px 5px;">
            <span>Raiser Code/Name</span>
            <br />
            <input type="text"
                   class="textboxStyleBig" value="@Model.CBFDoc.Raiser" style="width:100%; " />
        </td>
    </tr>
    <tr>
        <td style="width:50%; padding:0px 5px;">
            <span>Request For</span>
            <br />
            <input type="text" value="@Model.CBFDoc.RequestFor"
                   class="textboxStyleBig" style="width:100%; " />
