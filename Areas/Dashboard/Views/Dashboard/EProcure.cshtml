            <span>Branch</span>
            <br />
            <input type="text"
                   class="textboxStyleBig" value="@Model.CBFDoc.Branch" style="width:100%; " />
        </td>
    </tr>
    @if (Model.CBFDoc.ProductList != null && Model.CBFDoc.ProductList.Count > 0)
    {
    <tr>
        <td colspan="2" style="width: 100%; padding-top: 15px; padding-bottom: 15px;">
            <table style="width:100%;" cellpadding="0" cellspacing="0">
                <tr>
                    <th style="min-width: 80px; max-width: 80px; padding-left: 10px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        Prod/Service
                    </th>
                    <th style="min-width: 180px; max-width: 180px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        Product Group
                    </th>
                    <th style="min-width: 180px; max-width: 180px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        Product Name
                    </th>
                    <th style="min-width: 180px; max-width: 180px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        Description
                    </th>
                    <th style="min-width: 80px; max-width: 80px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        UOM
                    </th>
                    <th style="min-width: 80px; max-width: 80px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        Qty
                    </th>
                    <th style="min-width: 100px; max-width: 100px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        Amount
                    </th>
                    <th style="min-width: 100px; max-width: 100px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        GL Code
                    </th>
                    <th style="min-width: 140px; max-width: 140px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        CC
                    </th>
                    <th style="min-width: 100px; max-width: 100px; text-align: left; border-bottom: 1px solid black; border-top: 1px solid black; ">
                        Budget Line
                    </th>
                </tr>

