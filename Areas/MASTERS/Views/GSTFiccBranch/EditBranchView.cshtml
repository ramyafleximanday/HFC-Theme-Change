                            var stylesSelect = $('#txtdistrict');
                            stylesSelect.attr('disabled', false);
                            $("#txtdistrict").append("<option value=" + data1[i].district_gid + " >" + data1[i].dictrictcode + "</option>");
                        }
                    }
                }
                if (data.Data2 != "" && data.Data2 != null)
                {
                    var data2 = JSON.parse(data.Data2);
                    document.getElementById("txtgsttin").value = data2[0].branchgst_gstin;
                }
                if (data.length == 0) {
                    var stylesSelect = $('#txtdistrict');
                    stylesSelect.attr('disabled', true);
                    $('#btnsave').attr('disabled', true);
                }
            },
            error: function (result) {
                jAlert("Error.", "Error");
