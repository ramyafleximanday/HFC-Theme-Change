    $("#Pincode_id").change(function () {

        $("#txtgsttin").val("");
        $("#txtstate").empty();
        $("#txtdistrict").empty();
        var brandID = $("#Pincode_id").val();
        var Pincode_model =
       {
           "pincode_gid": brandID
       };
        $.ajax({
            url: '../GSTFiccBranch/Getstatebypincode/',
            data: JSON.stringify(Pincode_model),
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                if (data.Data1 != "" && data.Data1 != null) {
                    var data1 = JSON.parse(data.Data1);
                    $("#txtstate").empty();
                    if (data1.length > 0) {
                        $("#txtstate").append("<option value='0' >--Select--</option>");
                        for (var i = 0; i < data1.length; i++) {
                            var stylesSelect = $('#txtstate');
                            stylesSelect.attr('disabled', false);
                            $("#txtstate").append("<option value=" + data1[i].state_gid + " >" + data1[i].state_code + "</option>");
                        }
                    }
                }

                if (data.length == 0) {
                    var stylesSelect = $('#txtstate');
                    stylesSelect.attr('disabled', true);
                    $('#btnsave').attr('disabled', true);
                }
            },
            error: function (result) {
                jAlert("Error.", "Error");
                return false;
            }
        });
    });




    $("#txtstate").change(function () {

        $("#txtgsttin").val("");
        $("#txtdistrict").empty();
        var brandID = $("#txtstate").val();
        var brandID1 = $("#Pincode_id").val();
        var Pincode_model =
       {
           "state_gid": brandID,
           "pincode_gid": brandID1
       };
        $.ajax({
            url: '../GSTFiccBranch/Getdistrictbystate/',
            data: JSON.stringify(Pincode_model),
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            success: function (data) {
                if (data.Data1 != "" && data.Data1 != null) {
                    var data1 = JSON.parse(data.Data1);
                    $("#txtdistrict").empty();
                    if (data1.length > 0) {
                        $("#txtdistrict").append("<option value='0' >--Select--</option>");
