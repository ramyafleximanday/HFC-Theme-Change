        color: #EA071C !important;
    }
</style>
<script type="text/javascript">
    var _currDTTab = 0, _currPATab = 0, _currDOTTab = 0;
    $(function () {
        $("#DTtabs").tabs({
            select: function (e, i) {
                _currDTTab = i.index;
            }
        });

        $("#PAtabs").tabs({
            select: function (e, i) {
                _currPATab = i.index;
            }
        });

        $("#DOTtabs").tabs({
            select: function (e, i) {
                _currDOTTab = i.index;
            }
        });
    });
</script>
<div class="pageHeadingLayout">
    <span class="pageHeading" id="lblECF">ECF Maker</span>
</div>
<div class="well">
    <table class="myTableStyleTabNew" style="width:100%;">
        <tr>
            <td width="20%" style="padding: 0px 10px 0px 3px;">
                <span>Doc Number</span>
                <br />
                <input type="text"
                       id="txtSDocNumber"
                       name="txtSDocNumber"
                       value=""
                       class="fstextbox" />
            </td>
            <td width="20%" style="padding: 0px 10px">

                <span>Invoice Number</span>
                <br />
                <input type="text"
                       id="txtSInvoiceNumber"
                       name="txtSInvoiceNumber"
                       value=""
                       class="fstextbox" />
            </td>
            <td width="20%" style="padding: 0px 10px;">
                <span>Document Type</span>
                <br />
                <select name="ddlSDocType" id="ddlSDocType" class="form-control"
                        data-bind="options: DocumentTypeArray, optionsText: 'Text', optionsValue:'Id', optionsCaption:'- Select One -'"></select>
            </td>
            <td width="20%" style="padding: 0px 10px;">
                <span>Amount</span>
                <br />
                <input type="text"
                       name="txtSAmount"
                       id="txtSAmount"
                       value=""
                       maxlength="8"
                       @*onkeypress="return isNumberKey(event)"*@
                       class="fstextbox" />
            </td>
            <td style="width:20%; padding:0px; vertical-align:bottom;"></td>
        </tr>
        <tr>
            <td width="20%" style="padding: 0px 10px 0px 3px;">
                <span>Employee Code</span>
                <br />
                <input type="hidden" name="hdfSEmpCodeId" id="hdfSEmpCodeId" value="0" />
