<script type="text/javascript">

    var _currDTTab = 0, _currIVTab = 0;
    $(function () {

        $("#Maintabs").tabs({
            select: function (e, i) {

                _currDTTab = i.index;

            }
        });

        $("#Invoicetabs").tabs({
            select: function (e, i) {
                _currIVTab = i.index;

            }
        });
    });

    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;
        return true;
    }

</script>
