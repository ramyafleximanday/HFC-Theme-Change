@{
    ViewBag.Title = "Debit";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/Rmax_Site.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>
<style>
    table, th, td {
        white-space: nowrap;
    }
</style>
<div class="pageHeadingLayout">
    <span class="pageHeading">Debit Report</span>
</div>
<div class="well">
    <table class="myTableStyleTabNew" style="width:100%;">
        <tr>

            <td width="25%" style="padding: 0px 10px">
                <span>Processed Date From</span>
                <br />
                <input type="text"
                       id="txtDateFrom"
                       class="fstextbox " />
            </td>
            <td width="25%" style="padding: 0px 10px">
                <span>Processed Date To</span>
                <br />
                <input type="text"
                       id="txtDateTo"
                       class="fstextbox " />
            </td>
            <td width="25%" style="padding: 0px 10px">
                <span>ECF No</span>
                <br />
                <input type="text"
                       id="txtEcfNo"
                       class="fstextbox" />
            </td>
            <td width="25%" style="padding: 0px 10px">
                <span>Raiser</span>
                <br />

                <input type="text"
                       id="txtRaiser"
                       class="fstextbox" />
            </td>
        </tr>
        <tr>
            <td width="25%" style="padding: 0px 10px;">
                <span>Vendor</span>
                <br />

                <input type="text"
                       id="txtVendor"
                       class="fstextbox" />
            </td>
            <td width="25%" style="padding: 0px 10px;">
                <span>GL Code</span>
                <br />

                <input type="text"
                       id="txtGL"
                       class="fstextbox" />
            </td>
            <td width="25%" style="padding: 0px 10px;">
                <span>Location</span>
                <br />

                <input type="text"
                       id="txtLoc"
                       class="fstextbox" />
            </td>
            <td width="25%" style="padding: 0px 10px; vertical-align:bottom; text-align:right;">
                <button class="btn btn-primary" type="button" style="margin-top:10px;  color:white;" data-bind="click: function() { $root.DownloadDebitReport(); }, visible: DebitReportArray().length > 0">
                    <span style="color:white;" class="glyphicon glyphicon-download-alt"></span>
                    Export Excel
                </button>
                <button class="btn btn-primary" type="button" style="margin-top:10px;  color:white;" data-bind="click: function() { $root.SearchDebitReport(); }">
                    <span style="color:white;" class="glyphicon glyphicon-search"></span>
                    Search
                </button>
                <button class="btn btn-primary" type="button" style="margin-top:10px;  color:white;" data-bind="click: function() { $root.ClearDebitReport(); }">
                    <span style="color:white;" class="glyphicon glyphicon-refresh"></span>
                    Clear
                </button>
            </td>
        </tr>
    </table>

    <div class="table-responsive" style="margin-top: 15px;">
        <table id="gvReport" class="tableSmall table-bordered table-hover" data-bind="visible: DebitReportArray().length > 0">
            <thead>
                <tr>
                    <th align="center" style="padding-right:20px;">
                        Sl No
                    </th>
                    <th style="padding-right:20px;">
                        ECF No
                    </th>
                    <th style="padding-right:20px;">
                        Vendor Code
                    </th>
                    <th style="padding-right:20px;">
                        Vendor Name
                    </th>
                    <th style="padding-right:20px;">
                        Employee Code
                    </th>
                    <th style="padding-right:20px;">
                        Employee Name
                    </th>
                    <th style="padding-right:20px;">
                        Authorizer ID
                    </th>
                    <th style="padding-right:20px;">
                        Invoice No
                    </th>
                    <th style="padding-right:20px;">
                        Num Bill
                    </th>
                    <th style="padding-right:20px;">
                        Bill Start Date
                    </th>
                    <th style="padding-right:20px;">
                        Bill End Date
                    </th>
                    <th style="padding-right:20px;">
                        Invoice Date
                    </th>
                    <th style="padding-right:20px;">
                        Bill Amount
                    </th>
                    <th style="padding-right:20px;">
                        Service Taxable Amount
                    </th>
                    <th style="padding-right:20px;">
                        Service Tax
                    </th>
                    <th style="padding-right:20px;">
                        Expense month
                    </th>
                    <th style="padding-right:20px;">
                        CBS Amount
                    </th>
                    <th style="padding-right:20px;">
                        BS
                    </th>
                    <th style="padding-right:20px;">
                        CC
                    </th>
                    <th style="padding-right:20px;">
                        Product Code
                    </th>
                    <th style="padding-right:20px;">
                        Branch
                    </th>
                    <th style="padding-right:20px;">
                        Expense Code
                    </th>
                    <th style="padding-right:20px;">
                        SubCategory Code
                    </th>
                    <th style="padding-right:20px;">
                        Account
                    </th>
                    <th style="padding-right:20px;">
                        Narration
                    </th>
                </tr>
            </thead>
            <tbody data-bind="foreach:{ data: DebitReportArray, afterRender: DatatableDebit }, visible: DebitReportArray().length > 0">
                <tr>
                    <td align="center">
                        <span data-bind="text:$index()+1"></span>
                    </td>
                    <td>
                        <span data-bind="text: ECFNo"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: VendorCode"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: Vendorname"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: EmpCode"></span>
                    </td>
                    <td>
                        <span data-bind="text: EmpName"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: Authid"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: InvoiceNo"></span>
                    </td>
                    <td align="center">
                        <span data-bind="text: NumBill"></span>
                    </td>
                    <td align="center">
                        <span data-bind="text: BillStartDate"></span>
                    </td>
                    <td align="center">
                        <span data-bind="text: BillEndDate"></span>
                    </td>
                    <td align="center">
                        <span data-bind="text: InvoiceDate"></span>
                    </td>
                    <td align="right">
                        <span data-bind="text: BillAmount"></span>
                    </td>
                    <td align="right">
                        <span data-bind="text: SerTaxbleAmt"></span>
                    </td>
                    <td align="right">
                        <span data-bind="text: SerTax"></span>
                    </td>
                    <td>
                        <span data-bind="text: Expmonth"></span>
                    </td>
                    <td align="right">
                        <span data-bind="text: CBSAmount"></span>
                    </td>

                    <td align="left">
                        <span data-bind="text: BS"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: CC"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: ProductCode"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: Branch"></span>
                    </td>
                    <td>
                        <span data-bind="text: Expcode"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: Subcode"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: Account"></span>
                    </td>
                    <td align="left">
                        <span data-bind="text: Narration"></span>
                    </td>

                </tr>
            </tbody>
        </table>
    </div>


</div>
<script>
    var MUrlDet = "@Url.Action("GetBankSummary", "Reports")"
    var MUrlHelper = "@Url.Action("GetAutoCompleteCourier", "Helper")"
</script>
<script src="@Url.Content("~/Areas/FLEXISPEND/FS_UserScript/jsReports.js")" type="text/javascript"></script>

