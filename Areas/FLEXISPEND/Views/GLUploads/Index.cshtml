@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/Rmax_Site.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>
<style>
    table, th, td {
        white-space: nowrap;
    }
</style>
<script type="text/javascript">
    var _currDTTab = 0;
    $(function () {
        $("#DTtabs").tabs({
            select: function (e, i) {
                _currDTTab = i.index;
            }
        });
    });
</script>
<div class="pageHeadingLayout">
    <span class="pageHeading">GL Uploads</span>
</div>
<div class="well">
    <div id="DTtabs" style="font-size:12px;">
        <ul>
            <li><a href="#tabs-1">GL Upload</a></li>
            <li><a href="#tabs-2">GL Reupload</a></li>
        </ul>
        <div>
            <div id="tabs-1" style="height:180px;">
                <table width="50%" class="myTableStyleTabNew">
                    <tr>
                        <td style="width:50%; padding:0px 5px; position:relative;">
                            <span>Upload From</span>
                            <br />
                            <input type="text" id="txtUploadFrom" class="fstextbox fsDatePicker required" onkeypress="return isEvent(event)" />
                        </td>
                        <td style="width:50%; padding:0px 5px;">
                            <span>Upload To</span>
                            <br />
                            <input type="text" id="txtUploadTo" class="fstextbox fsDatePicker required" onkeypress="return isEvent(event)" />
                        </td>
                    </tr>
                    <tr>
                        <td style="width:50%; padding:0px 5px; position:relative;">
                            <span>Upload Type</span>
                            <br />
                            <select name="ddlUploadType" id="ddlUploadType" class="form-control required"
                                    data-bind="options: UploadTypeArray, optionsText: 'Text', optionsValue:'Id', optionsCaption:'- Select One -'"></select>

                        </td>
                        <td style="width:50%; padding:0px 5px; position:relative;">
                            <span>Batch No</span>
                            <br />
                            <input type="text" id="txtBatchNo" class="fstextbox" disabled="disabled" />
                        </td>
                    </tr>
                    <tr>
                        <td colspan="2" style="width:100%; padding:0px 5px; position:relative; text-align:right;">
                            <div>
                                <br />
                                <button type="button" accesskey="s" class="btn btn-primary" id="btnGLSubmit" data-bind="click:$root.GLSave">
                                    <span class="glyphicon glyphicon-save" style="color:white;"></span>
                                    Submit
                                </button>
                                <button type="button" accesskey="c" class="btn btn-primary" data-bind="click:$root.GLClear">
                                    <span class="glyphicon glyphicon-refresh" style="color:white;"></span>
                                    Clear
                                </button>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
            <div id="tabs-2" style="height:180px;">
                <table width="60%" class="myTableStyleTabNew">
                    <tr>
                        <td style="width:50%; padding:0px 5px; position:relative;">
                            <span>Upload Date</span>
                            <br />
                            <input type="text" id="txtCUploadDate" class="fstextbox fsDatePicker required" data-bind="event: {change: dateChange }" onkeypress=" return isevent(event)" />
                        </td>
                        <td style="width:50%; padding:0px 5px; position:relative;">
                            <span>Upload Type</span>
                            <br />
                            <select name="ddlCUploadType" id="ddlCUploadType" class="form-control required"
                                    data-bind="options: CUploadTypeArray, optionsText: 'Text', optionsValue:'Id', optionsCaption: '-Select One-', value:0, event:{ change:$root.UploadTypeSelectChanged }"></select>
                        </td>
                    </tr>
                    <tr>
                        <td style="width:50%; padding:0px 5px; position:relative;">
                            <span>Batch No</span>
                            <br />
                            <select name="ddlCBatchNo" id="ddlCBatchNo" class="form-control required"
                                    data-bind="options: CBatchNoArray, optionsText: 'Text', optionsValue:'Id', optionsCaption: '-Select One-', value:0"></select>
                        </td>
                        <td style="width:50%; padding:0px 5px; position:relative; text-align:right;">
                            <div>
                                <br />
                                <button type="button" accesskey="r" class="btn btn-success" id="btnRGSubmit" data-bind="click:$root.GLRegenerate">
                                    <span class="glyphicon glyphicon-transfer" style="color:white;"></span>
                                    Re-Generate
                                </button>
                                @*<button type="button" class="btn btn-danger" id="btnCSubmit" data-bind="click:$root.GLDelete">
                                    <span class="glyphicon glyphicon-remove" style="color:white;"></span>
                                    Delete
                                </button>*@
                                <button type="button" accesskey="c" class="btn btn-primary" data-bind="click:$root.CClear">
                                    <span class="glyphicon glyphicon-refresh" style="color:white;"></span>
                                    Clear
                                </button>
                            </div>
                        </td>
                    </tr>
                    <tr></tr>
                </table>
            </div>
        </div>
    </div>
</div>
<script>
    var UrlDet = "@Url.Action("DownloadExcel", "GLUploads")"
    var UrlHelper = "@Url.Action("GetAutoCompleteCourier", "Helper")"
</script>
<script src="@Url.Content("~/Areas/FLEXISPEND/FS_UserScript/jsGLUploads.js")" type="text/javascript"></script>
