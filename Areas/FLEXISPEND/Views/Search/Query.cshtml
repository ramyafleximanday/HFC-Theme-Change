@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/Content/Rmax_Site.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>
<style>
    table, th, td {
        white-space: nowrap;
    }
</style>
<div class="pageHeadingLayout">
    <span class="pageHeading">Query</span>
</div>
<div class="well">
    <table class="myTableStyleTabNew" style="width:100%;">
        <tr>
            <td width="16%" style="padding: 0px 10px">
                <span>Document Number</span>
                <br />
                <input type="text"
                       id="txtSECFNumber"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>Invoice Number</span>
                <br />
                <input type="text"
                       id="txtSInvoiceNumber"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>Raiser</span>
                <br />
                <input type="hidden" id="hdfRaiser" value="0" />
                <input type="text"
                       id="txtRaiser"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>Vendor</span>
                <br />
                <input type="hidden" id="hdfVendor" value="0" />
                <input type="text"
                       id="txtVendor"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>Employee</span>
                <br />
                <input type="hidden" id="hfEmployee" value="0" />
                <input type="text"
                       id="txtEmployee"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>Invoice Amount</span>
                <br />
                <input type="text"
                       id="txtSInvAmt"
                       @*onkeypress="return isNumberKey(event)"*@
                       class="fstextbox" />
            </td>

        </tr>
        <tr>
            <td width="16%" style="padding: 0px 10px">
                <span>PV Number</span>
                <br />
                <input type="text"
                       id="txtSPVNumber"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>ECF Date</span>
                <br />
                <input type="text"
                       id="txtSECFDate"
                       onkeypress="return isEvent(event)"
                       class="fstextbox fsDatePicker" />
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>Authorizer</span>
                <br />
                <input type="hidden" id="hdfAuthorizer" value="0" />
                <input type="text"
                       id="txtAuthorizer"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>ECF Amount</span>
                <br />
                <input type="text"
                       id="txtSECFAmount"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>PV Date</span>
                <br />
                <input type="text"
                       id="txtPVDate"
                       onkeypress="return isEvent(event)"
                       class="fstextbox fsDatePicker" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>Inward AWB No</span>
                <br />
                <input type="text"
                       id="txtInwAwbNo"
                       class="fstextbox" />
            </td>


        </tr>
        <tr>
            <td width="16%" style="padding: 0px 10px;">
                <span>Cheque AWB No</span>
                <br />
                <input type="text"
                       id="txtChqAwbNo"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>Cheque No</span>
                <br />
                <input type="text"
                       id="txtChqueNo"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>Cheque Amount</span>
                <br />
                <input type="text"
                       id="txtChqueAmount"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>Document Type</span>
                <br />
                <select id="ddlDocType" class="fstextbox"
                        data-bind="options: DocumentTypeArray, optionsText: 'Text', optionsValue:'Id', value:0, optionsCaption:'-Select One-'"></select>
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>Physical Receipt Date From</span>
                <br />
                <input type="text"
                       id="txtPDateFrom"
                       onkeypress="return isEvent(event)"
                       class="fstextbox fsDatePicker" />
            </td>
            <td width="16%" style="padding: 0px 10px">
                <span>Physical Receipt Date To</span>
                <br />
                <input type="text"
                       id="txtPDateTo"
                       onkeypress="return isEvent(event)"
                       class="fstextbox fsDatePicker" />
            </td>
        </tr>
        <tr>
            <td width="16%" style="padding: 0px 10px;">
                <span>CBF No</span>
                <br />
                <input type="text"
                       id="txtcbfno"
                       class="fstextbox" />
            </td>
            <td width="16%" style="padding: 0px 10px;">
                <span>PO No</span>
                <br />
                <input type="text"
                       id="txtpo"
                       class="fstextbox" />
            </td>
            </tr>
<tr>
            <td colspan="6" style="padding:0px 10px; vertical-align:bottom; text-align:right;">
                <button class="btn btn-primary" accesskey="e" type="button" style="margin-top:10px;  color:white;" data-bind="click: function() { $root.DownloadqueryReport(); }, visible: QueryArray().length > 0">
                    <span style="color:white;" class="glyphicon glyphicon-download-alt"></span>
                    Export Excel
                </button>
                <button class="btn btn-primary" accesskey="q" type="button" data-bind="click: $root.Search" style="margin-top:10px;  color:white;">
                    <span style="color:white;" class="glyphicon glyphicon-search"></span>
                    Search
                </button>
                <button class="btn btn-primary" accesskey="c" data-bind="click: $root.Clear" type="button" style="margin-top:10px;  color:white;">
                    <span style="color:white;" class="glyphicon glyphicon-refresh"></span>
                    Clear
                </button>
            </td>
        </tr>
    </table>

    <div class="table-responsive" style="overflow:auto; height:500px; margin-top: 15px;">
        <table id="tblFSQuery" class="tableSmall table-bordered table-hover" data-bind="visible: QueryArray().length > 0">
            <thead>
                <tr>
                    <th style="padding-left:10px; padding-right:10px;">
                        SLNo
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Document No
                    </th>
                    <th style="padding-left:10px; padding-right:10px;" class="colDate">
                        Document Date
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Document Type
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Document Amount
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Type
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Supplier/Employee Code
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Supplier/Employee Name
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Raiser By
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        PO Type
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Inward No
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Document Batch No
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Document Status
                    </th>
                    <th style="padding-left:10px; padding-right:10px;">
                        Process Status
                    </th>
                </tr>
            </thead>
            <tbody data-bind="foreach: { data: QueryArray, afterRender: DatatableCall }, visible: QueryArray().length > 0">
                <tr>
                    <td align="center">
                        <span data-bind="text: SlNo"></span>
                    </td>
                    <td>
                        <span title="View Details" style="color: blue; cursor:pointer; text-decoration:none;" data-bind="click: $root.viewECF"><span data-bind="text: DocNo"></span></span>
                    </td>
                    <td align="center">
                        <span data-bind="text: DocDate"></span>
                    </td>
                    <td>
                        <span data-bind="text: DocType"></span>
                    </td>
                    <td>
                        <span data-bind="text: DocAmount"></span>
                    </td>
                    <td>
                        <span data-bind="text: Type"></span>
                    </td>
                    <td>
                        <span data-bind="text: Code"></span>
                    </td>
                    <td>
                        <span data-bind="text: Name"></span>
                    </td>
                    <td>
                        <span data-bind="text: RaisedBy"></span>
                    </td>
                    <td>
                        <span data-bind="text: PoType"></span>
                    </td>
                    <td>
                        <span data-bind="text: InwardNo"></span>
                    </td>
                    <td>
                        <span data-bind="text: DocBatchNo"></span>
                    </td>
                    <td>
                        <span data-bind="text: DocumentStatus"></span>
                    </td>
                    <td>
                        <span data-bind="text: ProcessStatus"></span>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>

    @*Dialog Box*@
    <div style="display:none" id="Dialog">

    </div>
</div>
<script>
    //$(document).ready(function () {
    //    $('#tblFSQuery').dataTable().css("width", "99%");
    //});
    var MUrlDet = "@Url.Action("GetQuery", "Search")"
    var MUrlHelper = "@Url.Action("GetAutoCompleteCourier", "Helper")"
</script>
<script src="@Url.Content("~/Areas/FLEXISPEND/FS_UserScript/jsQuery.js")" type="text/javascript"></script>
