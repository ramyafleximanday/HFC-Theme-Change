                               '</tr>';
                $("#shipmentgrid tbody").append(newRow);
                $.ajax({
                    url: '@Url.Action("getshipmentlist", "PoRaiserEdit")',
                    data: '{}',
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8',
                    success: function (data) {
                        if (data == 0) {
                            jAlert("Error Occured", "Error", function () {
                                return false;
                            });
                            return false;
                        }
                        else {
                            $("#" + shipmenttype + "").empty();
