                            return false;
                        });
                        return false;
                    }
                    else {
                        $("#shipmentEdit").load('@Url.Action("shipmentsave","PoRaiserEdit")')
                    }
                },
                error: function () {
                    jAlert("something seems wrong", "Error");
                }

            });
        });

    });

