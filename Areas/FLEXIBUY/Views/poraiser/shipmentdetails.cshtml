        if (parseInt(qty) == parseInt(actQty)) {
            jAlert("Shipped Quantity equals to the Actual Quantity", "Error");
            return false;
        }
        else
        {
            $('#DownloadTemplate').show();
            $('#fileuploadprocess').show();
            //$("#btn_add").attr("disabled", true);
            //$(".excel").attr("disabled", true);
        }
    }

