@model  IEM.Areas.EOW.Models.EOW_EmployeeeExpense
@using IEM.Areas.EOW.Models
@{
    EOW_EmployeeeExpense sh = (EOW_EmployeeeExpense)ViewBag.EOW_EmployeeeExpenseheader;

}
<link href="@Url.Content("~/Content/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-ui-1.8.24.min.js")" type="text/javascript"></script>



@using (Html.BeginForm())
{
    <div class="wellNew">
        <div class="input-group ">
            <table class="myTableStyleTabNew">
                <tr>
                    <td>
                        <span>Raiser Mode</span><br />
                        @Html.DropDownListFor(n => sh.raisermodeId, sh.Raiser_Modedata, new { @class = "textboxStyle ", @id = "raisermodeName", @disabled = "disabled" })
                    </td>
                    <td>
                        <span>Raiser Code</span><br />
                        @Html.TextBoxFor(n => sh.Raiser_Code, new { disable = true, @class = "textboxStyle", @id = "txtraisercode", @disabled = "disabled" })
                    </td>
                    <td>
                        <span>Raiser Name</span><br />
                        @Html.TextBoxFor(n => sh.Raiser_Name, new { disable = true, @class = "textboxStyle", @id = "txtraisername", @disabled = "disabled" })
                    </td>
                    <td>
                        <span>ECF Date</span><br />
                        @Html.TextBoxFor(n => sh.ECF_Date, new { disable = true, @class = "textboxStyle ", @id = "txtecfdate", @disabled = "disabled" })
                    </td>
                </tr>
                <tr>
                    <td>
                        <span>Grade</span><br />
                        @Html.TextBoxFor(n => sh.Grade, new { disable = true, @class = "textboxStyle", @id = "txtgrade", @disabled = "disabled" })
                    </td>
                    <td>
                        <span>Claim  Month</span><br />
                        @Html.TextBoxFor(n => sh.ClaimMonth, new { disable = true, @class = "textboxStyle ", @id = "txtclaimmonth", @disabled = "disabled" })
                    </td>
                    <td>
                        <span>ECF  Amount</span><br />
                        @Html.TextBoxFor(n => sh.ECF_Amount, new { disable = true, @class = "textboxStyle ", @id = "txtecfamount", @disabled = "disabled" })

                    </td>
                    <td>
                        <span>&nbsp;</span><br />&nbsp;&nbsp;&nbsp;
                    </td>
                </tr>
            </table>
        </div>
    </div>
    }

