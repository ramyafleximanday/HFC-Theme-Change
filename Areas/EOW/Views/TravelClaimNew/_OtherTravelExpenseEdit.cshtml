@model  IEM.Areas.EOW.Models.EOW_TravelClaim
@using IEM.Areas.EOW.Models
@{
    EOW_TravelClaim sh = (EOW_TravelClaim)ViewBag.TravelClaimheader;
}
@{
    var viewfor = (string)ViewBag.viewfor;
    if (viewfor == "edit")
    {
        <div id="EditExpenseqwer">

            @using (Html.BeginForm())
            {
                <div style="padding:0;margin:0;">
                    <input type="hidden" id="ecfamomtssessiontmte" value="@Session["Ecfamountvaltmeditoe"]" />

                    <table class="myTableStyleTabNew" style="width: 100%;">
                        <tr>
                            <td style="width: 33%;">
                                <span>Nature of Expenses</span><br />
                                @Html.DropDownListFor(model => sh.NatureofExpensesId, sh.ExpNatureofExpdata, new { @class = "textboxStyle", @id = "ddlNatureofExpensesto" })
                            </td>
                            <td style="width: 33%;">
                                <span>Expense Category</span><br />
                                @Html.DropDownListFor(model => sh.ExpenseCategoryId, sh.ExpCatdata, new { @class = "textboxStyle", @id = "ddlExpenseCategoryto" })
                            </td>
                            <td style="width: 35%;">
                                <span>Sub Category</span><br />
                                @Html.DropDownListFor(model => sh.SubCategoryId, sh.ExpSubCatdata, new { @class = "wht textboxStyle", @id = "ddlSubCategoryto" })
                            </td>
                            <td style="width:10%">
                                <span>&nbsp;</span><br />
                                <input type="button" value="?" style="margin-top: 5px;" id="Getpolicypopup" onclick="Newpolicypopup()" />
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span>Place From</span><br />
                                @Html.DropDownListFor(model => sh.PlaceFrom, sh.Citydata, new { @class = "textboxStyle", @id = "ddlPlaceFromto" })
                            </td>
                            <td>
                                <span>Claim Period From</span><br />
                                @Html.TextBoxFor(model => sh.ClaimPeriodFrom, new { disable = true, @class = "textboxStyle", @id = "txtClaimPeriodFromto" })
                            </td>
                            <td colspan="2">
                                <span>Claim Period To</span><br />
                                @Html.TextBoxFor(model => sh.ClaimPeriodTo, new { disable = true, @class = "textboxStyle", @id = "txtClaimPeriodToto" })
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span>FC</span><br />
                                @Html.DropDownListFor(model => sh.FC, sh.Exp_FCC, new { @class = "textboxStyle ", @id = "txtFCto" })
                            </td>
                            <td>
                                <span>CC</span><br />
                                @Html.DropDownListFor(model => sh.CC, sh.Exp_CCC, new { @class = "textboxStyle ", @id = "txtCCto" })
                            </td>
                            <td colspan="2">
                                <span>FCCC Code</span><br />
                                @Html.TextBoxFor(model => sh.Exp_FCCC, new { disable = true, @class = "textboxStyle", @id = "txtFCCC", @maxlength = "58", @disabled = "disabled" })
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span>Product Code</span><br />
                                @Html.TextBoxFor(model => sh.ProductCode, new { disable = true, @class = "textboxStyle", @id = "txtProductCodeto" })
                            </td>
                            <td>
                                <span>OUCode</span><br />
                                @Html.TextBoxFor(model => sh.OUCode, new { disable = true, @class = "textboxStyle", @id = "txtOUCodeto" })

                            </td>
                            <td colspan="2">
                                <span>Amount</span><br />
                                @Html.TextBoxFor(model => sh.Amount, new { disable = true, @class = "textboxStyle", @id = "txtAmountto" })

                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <span>Description</span><br />
                                @Html.TextAreaFor(model => sh.travelDescription, new { disable = true, @class = "textboxStyle txtarea", @id = "txttravelDescription", @maxlength = "250" })
                                @if (viewfor == "edit")
                                {
                                    <div>
                                        <script type="text/javascript">
                                            $(document).ready(function () {
                                                hideProgress();
                                            });
                                        </script>
                                    </div>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td style="padding-top: 10px;" colspan="4" align="center">
                                <center>
                                    <input type="button" id="btnsavexpu" class="btn btn-primary" value="Save" onclick="SaveotherTravelmode1()" />
                                    <input type="button" id="btncanexpu" class="btn btn-warning" value="Cancel" onclick="GotoIndexotherExpense()" />
                                </center>
                            </td>
                        </tr>
                    </table>
                </div>
            }
        </div>
    }
    else if (viewfor == "view")
    {
        <div id="EditExpense">

            @using (Html.BeginForm())
            {
                <div style="padding:0;margin:0;">
                    <input type="hidden" id="ecfamomtssessiontmy" value="@Session["Ecfamountvaltmeditoe"]" />

                    <table class="myTableStyleTabNew" style="width: 100%;">
                        <tr>
                            <td style="width: 33%;">
                                <span>Nature of Expenses</span><br />
