                            </td>
                            <td>
                                <span>Claim Period From</span><br />
                                @Html.TextBoxFor(model => sh.ClaimPeriodFrom, new { disable = true, @class = "textboxStyle", @id = "txtClaimPeriodFromto", @disabled = @ViewBag.viewmode })
                            </td>
                            <td colspan="2">
                                <span>Claim Period To</span><br />
                                @Html.TextBoxFor(model => sh.ClaimPeriodTo, new { disable = true, @class = "textboxStyle", @id = "txtClaimPeriodToto", @disabled = @ViewBag.viewmode })
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span>FC</span><br />
                                @Html.DropDownListFor(model => sh.FC, sh.Exp_FCC, new { @class = "textboxStyle ", @id = "txtFCto", @disabled = @ViewBag.viewmode })
                            </td>
                            <td>
                                <span>CC</span><br />
                                @Html.DropDownListFor(model => sh.CC, sh.Exp_CCC, new { @class = "textboxStyle ", @id = "txtCCto", @disabled = @ViewBag.viewmode })
                            </td>
                            <td colspan="2">
                                <span>FCCC Code</span><br />
                                @Html.TextBoxFor(model => sh.Exp_FCCC, new { disable = true, @class = "textboxStyle", @id = "txtFCCC", @maxlength = "58", @disabled = "disabled" })
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <span>Product Code</span><br />
                                @Html.TextBoxFor(model => sh.ProductCode, new { disable = true, @class = "textboxStyle", @id = "txtProductCodeto", @disabled = @ViewBag.viewmode })
                            </td>
                            <td>
                                <span>OUCode</span><br />
                                @Html.TextBoxFor(model => sh.OUCode, new { disable = true, @class = "textboxStyle", @id = "txtOUCodeto", @disabled = @ViewBag.viewmode })

                            </td>
                            <td colspan="2">
                                <span>Amount</span><br />
                                @Html.TextBoxFor(model => sh.Amount, new { disable = true, @class = "textboxStyle", @id = "txtAmountto", @disabled = @ViewBag.viewmode })

                            </td>
                        </tr>
                        <tr>
                            <td colspan="4">
                                <span>Description</span><br />
                                @Html.TextAreaFor(model => sh.travelDescription, new { disable = true, @class = "textboxStyle txtarea", @id = "txttravelDescription", @maxlength = "250", @disabled = @ViewBag.viewmode })
                                @if (viewfor == "view")
                                {
                                    <div>
                                        <script type="text/javascript">
                                            $(document).ready(function () {
                                                hideProgress();
                                            });
                                        </script>
                                    </div>
                                }
                            </td>
                        </tr>
                        <tr>
                            <td style="padding-top: 10px;" colspan="4" align="center">
                                <center>
                                    <input type="button" id="btncanexpu" class="btn btn-warning" value="Cancel" onclick="GotoIndexotherExpense()" />
                                </center>
                            </td>
                        </tr>
                    </table>
                </div>
            }
        </div>
    }

}

